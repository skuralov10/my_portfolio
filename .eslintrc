{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "comma-dangle": "off",
    "object-curly-spacing": "off",
    "require-jsdoc": "off"
    
  },
  "globals": {
    "$": true,
    "jQuery": true
  },
  "extends": ["eslint:recommended", "google"]
}
